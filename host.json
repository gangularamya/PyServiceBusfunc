{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },

  "extensions": {
    "serviceBus": {
      "maxConcurrentCalls": 8,
      "maxAutoRenewDuration": "01:10:00"
          }
      },
      "functionTimeout":"01:10:00"
    }
